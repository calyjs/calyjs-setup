{
	"name": "website",
	"root": "apps/website",
	"sourceRoot": "apps/website",
	"projectType": "application",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "!{projectRoot}/dist/**",
      "!{projectRoot}/node_modules/**",
      "!{projectRoot}/out-tsc/**",
      "!{projectRoot}/coverage/**",
      "!{projectRoot}/CHANGELOG.md",
      "!{projectRoot}/README.md"
    ],
    "production": [
      "{projectRoot}/src/**",
      "{projectRoot}/package.json",
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.*.json",
      "{projectRoot}/.babelrc.*",
      "{projectRoot}/api-extractor.json",
      "{projectRoot}/api-extractor.*.json",
      "{projectRoot}/rollup.config.*"
    ]
  },
	"targets": {
		"build": {
			"dependsOn": [
				"clear",
				"write-translations",
				"write-heading-ids",
				"^build"
			],
      "executor": "nx:run-commands",
			"options": {
				"cwd": "apps/website",
				"commands": [
					"docusaurus build"
				],
				"parallel": false
			},
			"inputs": ["default"],
  		"outputs": [
				"{projectRoot}/i18n",
				"{projectRoot}/build"
			],
			"cache": true
		}
	}
}
