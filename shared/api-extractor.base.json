{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "bundledPackages": [],
  "newlineKind": "lf",
  "enumMemberOrder": "preserve",
	"projectFolder": ".",
	"mainEntryPointFilePath": "<projectFolder>/out-tsc/src/index.d.ts",
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.lib.json",
    "overrideTsconfig": {
      "compilerOptions": {
        "moduleResolution": "Bundler",
        "target": "ES2022",
        "lib": ["DOM", "ES2015", "ES2022"],
        "types": ["node", "resize-observer-browser"],
        "skipLibCheck": true
      }
    }
  },
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-forgotten-export": {
        "logLevel": "warning"
      },
      "ae-incompatible-release-tags": {
        "logLevel": "warning"
      },
      "ae-internal-missing-underscore": {
        "logLevel": "warning"
      },
      "ae-internal-mixed-release-tag": {
        "logLevel": "warning"
      },
      "ae-undocumented": {
        "logLevel": "none"
      },
      "ae-unresolved-inheritdoc-reference": {
        "logLevel": "warning"
      },
      "ae-unresolved-inheritdoc-base": {
        "logLevel": "warning"
      },
      "ae-wrong-input-file-type": {
        "logLevel": "error"
      },
      "ae-missing-release-tag": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    }
  }
}